{
  "name": "aurelia-inspector",
  "description": "Aurelia 2 Inspector",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "???"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "aurelia": "2.0.0-beta.22"
  },
  "devDependencies": {
    "@aurelia/webpack-loader": "^2.0.0-beta.22",
    "@types/chrome": "0.0.290",
    "@types/jest": "^26.0.19",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "autoprefixer": "^10.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "htmlhint": "^0.14.2",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "postcss-loader": "^7.0.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "@aurelia/testing": "latest",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "@aurelia/vite-plugin": "latest",
    "vite": "^6.0.3",
    "vitest": "^2.1.8",
    "jsdom": "^25.0.1",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.18.1"
  },
  "engines": {
    "npm": ">=10.0.0 <11.0.0",
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "reload": "open http://reload.extensions/",
    "lint:js": "eslint src test --ext .js,.ts",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html",
    "pretest": "npm run lint",
    "start": "rimraf dist && webpack --watch",
    "build": "rimraf dist && webpack --env production",
    "analyze": "rimraf dist && webpack --env production --analyze",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "\\.(ts|html)$": "@aurelia/ts-jest"
    },
    "setupFiles": [
      "<rootDir>/test/setup.ts"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
